{
  "name": "ulink",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cap:sync": "npx cap sync",
    "cap:build": "npm run build && npx cap sync",
    "deploy:bundle": "npm run build && npx capawesome bundles:create --path dist",
    "deploy:dev": "npm run deploy:bundle && npx capawesome bundles:channels:assign development --bundle-id $(npx capawesome bundles:list --limit 1 --json | jq -r '.[0].id')",
    "deploy:staging": "npm run deploy:bundle && npx capawesome bundles:channels:assign staging --bundle-id $(npx capawesome bundles:list --limit 1 --json | jq -r '.[0].id')",
    "deploy:production": "npm run deploy:bundle && npx capawesome bundles:channels:assign production --bundle-id $(npx capawesome bundles:list --limit 1 --json | jq -r '.[0].id')"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.1",
    "@capacitor/app": "^8.0.1",
    "@capacitor/background-runner": "^3.0.0",
    "@capacitor/browser": "^8.0.1",
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "@capacitor/local-notifications": "^8.0.1",
    "@capawesome/capacitor-live-update": "^8.1.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@supabase/supabase-js": "^2.90.1",
    "@tanstack/react-query": "^5.90.16",
    "@tldraw/tldraw": "^4.3.1",
    "@types/three": "^0.182.0",
    "canvas-confetti": "^1.9.4",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^2.0.5",
    "react-image-crop": "^11.0.10",
    "react-router-dom": "^7.12.0",
    "react-youtube": "^10.1.0",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capawesome/cli": "^3.11.0",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0",
    "wrangler": "^4.67.0"
  }
}
